@model IEnumerable<FreightManagement.ViewModels.CarrierViewModel>

@{
    ViewBag.Title = "Listagem de Transportadoras";
}

<h2>@ViewBag.Title:</h2>

<p>
    @Html.RouteLink("Cadastrar Novo", "CarrierCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TradingName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cnpj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyName)
            </td>
            <td>
                @Html.DisplayFor(model => item.TradingName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cnpj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.RouteLink("Editar", "CarrierEdit", new { id = item.Id }) |
                @Html.RouteLink("Detalhes", "CarrierDetails", new { id = item.Id }) |
                @Html.RouteLink("Remover", "CarrierDelete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
